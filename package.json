    "description": "",
    "dependencies": {
        "body-parser": "^1.20.2",
        "cookie-parser": "^1.4.6",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "htmlfy": "^0.2.1",
        "lottie-web": "^5.12.2"
    },
    "devDependencies": {
        "chalk": "^5.3.0",
        "esbuild": "^0.23.0",
        "gulp": "^5.0.0",
        "gulp-sass": "^5.1.0",
        "jtsx-loader": "^0.1.3",
        "minimist": "^1.2.8",
        "nodemon": "^3.1.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "sass": "^1.77.8",
        "tsx": "^4.17.0",
        "ws": "^8.18.0"
    }

{
    "name": "ancros-jtsx-starter",
    "version": "0.1.1",
    "main": "index.js",
    "type": "module",
    "imports": {
        "#@/*": "./src/*"
    },
    "engines": {
        "node": ">=20.15.1"
    },
    "scripts": {
        "dev": "nodemon --config ./.ancros/nodemon.json --import jtsx-loader ./gulp.ancros.js dev",
        "debug": "nodemon --config ./.ancros/nodemon.json --import jtsx-loader ./gulp.ancros.js dev",
        "start": "node --import jtsx-loader ./gulp.ancros.js start",
        "build": "node --import jtsx-loader ./gulp.ancros.js build --prod",
        "server": "node --import jtsx-loader ./gulp.ancros.js server ./src/server/server.js",

        "test": "nodemon --config ./.ancros/nodemon.json --import jtsx-loader ./gulp.ancros.js test"
    },
    "keywords": [],
    "author": "Dergachev Mihail <dergachev.mihail@gmail.com>",
